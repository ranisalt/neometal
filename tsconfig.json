{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "none",
    "lib": ["DOM", "ES2019"],
    "outDir": "./lib",
    "rootDir": "./src",
    "removeComments": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
